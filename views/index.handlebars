<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>Eat Da Burger</h1>
    </div>
  </div>
  <div class="row">
    <div class="col-md-6">
      <h1>Burgers to Eat</h1>
       {{#each burgers}}
      {{#unless this.devoured}}
    <ul>
      {{this.id}}. {{this.burger_name}}
    </ul>
    {{/unless}}
    {{/each}}
    </div>
    <div class="col-md-6">
      <h1>Burgers that Were Eaten</h1>
       {{#each burgers}}
      {{#if this.devoured}}
    <ul>
      {{this.id}}. {{this.burger_name}}
    </ul>
    {{/if}}
    {{/each}}
    </div>
  </div>
</div>